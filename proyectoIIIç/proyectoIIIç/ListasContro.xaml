<UserControl x:Class="proyectoIIIç.ListasContro"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:proyectoIIIç"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel>
            <TextBlock Text="Listas Enlazadas" FontSize="20" FontWeight="Bold" Margin="10"/>
            <Button Content="Agregar Nodo" Click="BtnAgregarNodo_Click" Margin="5"/>
            <Button Content="Eliminar Nodo" Click="BtnEliminarNodo_Click" Margin="5"/>
            <ListBox x:Name="ListaEnlazada" Height="200" Margin="10"/>
        </StackPanel>
    </Grid>
</UserControl>
